package StringProgram;

public class ImmutableExample {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
String t="Delhi";
String o="Mumbai";	
String k="delhi";

String y=new String("Mumbai");
String l=new String("Delhi");
String p=new String("Hello");


System.out.println("string is created in string pool"+t);
System.out.println("string is created in string pool"+o);
System.out.println("string is created in string pool"+k);


System.out.println("string is created in  heap and refrence to string pool"+y);
System.out.println("string is created in  heap and refrence to string pool"+l);
System.out.println("string is created in  heap and string pool"+p);


if(o==l)
{
	System.out.println("string is equal");
}
else
{
	System.out.println("string is not equal");
}

if(y==p)
{
	System.out.println("string is equal");
}
else
{
	System.out.println("string is not equal");
}
if(t==o)
{
	System.out.println("string is equal");
}
else
{
	System.out.println("string is not equal");
}
if(k==y)
{
	System.out.println("string is equal");
}
else
{
	System.out.println("string is not equal");
}
if(p==y)
{
	System.out.println("string is equal");
}
else
{
	System.out.println("string is not equal");
}



if(o.equals(l))
{
	System.out.println("string is equal");
}
else
{
	System.out.println("string is not equal");
}
 if(y.equals(p))
 {
		System.out.println("string is equal");
	}
	else
	{
		System.out.println("string is not equal");
	}

 if(t.equals(o))
 {
		System.out.println("string is equal");
	}
	else
	{
		System.out.println("string is not equal");
	}

 if(k.equals(y))
 {
		System.out.println("string is equal");
	}
	else
	{
		System.out.println("string is not equal");
	}

 if(p.equals(y))
 {
		System.out.println("string is equal");
	}
	else
	{
		System.out.println("string is not equal");
	}

	}
}
